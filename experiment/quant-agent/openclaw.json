{
  "agents": {
    "defaults": {
      "workspace": "/app/workspace",
      "model": "${AGENT_MODEL}",
      "sandbox": {
        "mode": "off",
        "scope": "agent"
      },
      "heartbeat": {
        "every": "30m"
      }
    },
    "list": [
      {
        "id": "main",
        "default": true,
        "name": "QuantAgent",
        "workspace": "/app/workspace"
      }
    ]
  },
  "models": {
    "mode": "merge",
    "providers": {
      "custom": {
        "baseUrl": "${AGENT_BASE_URL}",
        "apiKey": "${AGENT_API_KEY}",
        "api": "${AGENT_API_TYPE}",
        "models": [
          {
            "id": "${AGENT_MODEL_ID}",
            "name": "${AGENT_MODEL_ID}"
          }
        ]
      }
    }
  },
  "cron": {
    "enabled": true,
    "maxConcurrentRuns": 1,
    "sessionRetention": "48h",
    "runLog": {
      "maxBytes": "5mb",
      "keepLines": 5000
    }
  },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "auth": {
      "mode": "token",
      "token": "${GATEWAY_AUTH_TOKEN}"
    }
  },
  "tools": {
    "web": {
      "search": {
        "enabled": true,
        "provider": "brave",
        "apiKey": "${BRAVE_API_KEY}"
      },
      "fetch": { "enabled": true }
    }
  },
  "channels": {
    "telegram": {
      "enabled": false
    }
  }
}
